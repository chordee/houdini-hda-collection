{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"detail"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int pt_num = npoints(1);
vector end_pos = vector(point(1, 'P', pt_num - 1));
vector start_pos = vector(point(1, 'P', 0));
vector dir = normalize(end_pos - start_pos);
float step = prim(1, 'restlength', 0) / float(pt_num);

f@step_distance = step; // detail attribute 

addpoint(0, start_pos); // first point

// get parameters 

int branch_limited = int(ch('branch_limited'));

//initial

vector pos[], dirs[];
int step_ages[], ids[], branch_step[], step_life[];
int mnt = 0; 
int branch = 0;

append(pos, start_pos);
append(dirs, dir);
append(step_ages, pt_num);
append(ids ,mnt);
append(branch_step, 0);
append(step_life, pt_num);

// set attributes

setpointattrib(0, 'id', 0, 0, 'set');  

//vector exp functiob

vector exp_vec(vector v; float e){     
    vector s;
    s.x = (v.x > 0)?1:-1;
    s.y = (v.y > 0)?1:-1;
    s.z = (v.z > 0)?1:-1;
    v = pow(abs(v), e) * s;
    return v; 
}

// here we go
 
for(int i = 1; i < pt_num; i++){
    int k = len(pos);

    for(int j = 0; j < k; j++){
    
        vector alt_dir = {0,0,0}; // initial alt_dir

        float bias = radians(abs(chf('bias' + itoa(branch_step[j]))));        
        float angle_exp = chf('angle_exp' + itoa(branch_step[j]));
        vector branch_dir = chv('branch_dir' + itoa(branch_step[j]));
        
        // get new angle
        
        if(j == 0){
            alt_dir = qrotate(point(1, 'orient', i), point(1, 'tangentu', i));     
        }else{
            vector noise_p = pos[j] * chv('freq' + itoa(branch_step[j])) + chv('offset' + itoa(branch_step[j]));
            vector temp_v = fit(vector(snoise(noise_p, chi('turb' + itoa(branch_step[j])), chf('rough' + itoa(branch_step[j])), chf('atten' + itoa(branch_step[j])))), vector(-.5), vector(.5), vector(-1), vector(1));
            temp_v = exp_vec(temp_v, angle_exp);
            temp_v = fit(temp_v, vector(-1), vector(1), vector(-bias), vector(bias));            
            vector4 temp_q = eulertoquaternion(temp_v, 0);
            vector tmp_branch_dir = lerp(dirs[j], branch_dir, float(chramp('branch_dir_ramp'  + itoa(branch_step[j]), 1 - float(step_ages[j]) / step_life[j])));
            alt_dir = qrotate(temp_q, tmp_branch_dir);
        }
        
        pos[j] += alt_dir * step; // move on
        
        int pt = addpoint(0, pos[j]); // add point
        
        // set attributes
        
        setpointattrib(0, 'id', pt, ids[j], 'set');
        setpointattrib(0, 'step', pt, i, 'set');
        setpointattrib(0, 'branch_step', pt, branch_step[j], 'set');   

        float step_phase = 1 - (float(step_ages[j]) / step_life[j]);

        float bias2 = chf('branch' + itoa(branch_step[j] + 1));
                        
        // Branch 

        vector seed = vector(chf('seed' + itoa(branch_step[j] + 1)) + 2);
        
        if((float(rand(pos[j] + seed) ) < bias2) && (step_ages[j] > 2)){
            
            if(step_phase > chf('start_at' + itoa(branch_step[j] + 1))){

                mnt++;
                int newpt = addpoint(0, pos[j]);
                setpointattrib(0, 'id', newpt, mnt, 'set'); 
                setpointattrib(0, 'step', newpt, i, 'set');
                setpointattrib(0, 'branch_step', newpt, branch_step[j]+1, 'set');
                append(pos, pos[j]);

                //get parameter

                int bi_branch = chi('bi_branch' + itoa(branch_step[j] + 1));

                float min_branch_root_bias_angle = abs(chf('min_branch_root_bias_angle' + itoa(branch_step[j] + 1)));
                float max_branch_root_bias_angle = abs(chf('max_branch_root_bias_angle' + itoa(branch_step[j] + 1)));
                float branch_root_bias_angle_exp = abs(chf('branch_root_bias_angle_exp' + itoa(branch_step[j] + 1)));

                int min_branch_step = chi('min_branch_step' + itoa(branch_step[j] + 1));
                int max_branch_step = chi('max_branch_step' + itoa(branch_step[j] + 1));

                float max_branch_root_open_angle = chf('max_branch_root_open_angle' + itoa(branch_step[j] + 1));
                float min_branch_root_open_angle = chf('min_branch_root_open_angle' + itoa(branch_step[j] + 1));
                float branch_root_open_angle_exp = chf('branch_root_open_angle_exp' + itoa(branch_step[j] + 1));

                float age_exp = chf('age_exp' + itoa(branch_step[j] + 1));

                // get new direction

                matrix3 mmm = ident();

                float angle = max_branch_root_bias_angle;
                        
                float randx = float(rand(pos[j] + seed));
                float tmp_randx = (randx - 0.5) * 2;
                tmp_randx = fit01(pow(abs(tmp_randx), branch_root_bias_angle_exp), min_branch_root_bias_angle/180, max_branch_root_bias_angle/180) ;
                randx = (randx > .5 )?tmp_randx:tmp_randx * -1;
                randx = randx / 2 + .5;

                float randy = float(rand((pos[j] + seed)* -1));
                float tmp_randy = (randy - 0.5) * 2;
                tmp_randy = fit01(pow(abs(tmp_randy), branch_root_open_angle_exp), min_branch_root_open_angle/180, max_branch_root_open_angle/180);
                randy = (randy > .5)?tmp_randy:tmp_randy * -1;
                randy = randy / 2 + .5;

                vector dir_parent = dirs[j];
                dir_parent = (max(abs(dir_parent.x), abs(dir_parent.z)) == 0)?{0, 0, 1}:normalize(set(dir_parent.x, 0, dir_parent.z));
                vector rotation;
                vector new_dir;
                vector bi_dir;

                if(dir_parent == {0, 0, 1}){
                    rotation = fit01(set(randx, randy, 0), set(-180, -180, 0), set(180, 180, 0));
                    mmm = maketransform(0, rotation);
                    new_dir = {0, 1, 0} * mmm;
                    mmm = maketransform(0, rotation * {1, -1, 1}); 
                    bi_dir = {0,1,0} * mmm; 
                }else{ 
                    matrix3 tmp_matrix; 
                    if(randx > 0.5){
                        tmp_matrix = lookat(dir_parent, {0,0,0}, {0,1,0});
                    }else{
                        tmp_matrix = lookat(-(dir_parent), {0,0,0}, {0,1,0});
                    }
                    rotation = fit01(set(randx, randy, 0), set(-180, -180, 0), set(180, 180, 0 ));
                    mmm = maketransform(0, rotation);
                    mmm *= tmp_matrix;
                    new_dir = {0,1,0} * mmm;
                    mmm = maketransform(0, rotation * {1, -1, 1});
                    mmm *= tmp_matrix;
                    bi_dir = {0,1,0} * mmm;
                }               
                append(dirs,  new_dir);



                // get new branch step age
 
                int age = int(fit01(pow(rand(pos[j]), age_exp), min_branch_step, max_branch_step));
                age = int(lerp( min_branch_step, max_branch_step, pow(chramp('step_ramp' + itoa(branch_step[j] + 1) , step_phase), age_exp)));

                // append attribute array
                
                append(step_ages, age);
                append(step_life, age);
                append(ids, mnt);
                append(branch_step, branch_step[j]+1);

                if(bi_branch == 1){
                    mnt++;
                    newpt = addpoint(0, pos[j]);
                    setpointattrib(0, 'id', newpt, mnt, 'set'); 
                    setpointattrib(0, 'step', newpt, i, 'set');
                    setpointattrib(0, 'branch_step', newpt, branch_step[j]+1, 'set');
                    append(pos, pos[j]);
                    append(dirs, bi_dir);
                    append(step_ages, age);
                    append(step_life, age);
                    append(ids, mnt);
                    append(branch_step, branch_step[j]+1);
                }
                
            }
        }
        
        step_ages[j] -= 1;
        
    }

    // stop growth

    k = len(pos);
    for(int j = 0; j < k; j++){
        if(step_ages[j] < 0){
            // remove frome array 
            removeindex(pos, j);
            removeindex(dirs, j);
            removeindex(step_ages, j);
            removeindex(ids, j);
            removeindex(branch_step, j);
            removeindex(step_life, j);
            j--;
            k--;
        }

    }
}

